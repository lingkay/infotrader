<!DOCTYPE html>
<html lang="en">
<head>
    <title>Infotrade</title>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=0">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="favicon.ico"/>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{ asset('bundles/coretemplate/assets/bootstrap/css/bootstrap.min.css') }}"/>

    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="{{ asset('bundles/coretemplate/assets/css/ie10-viewport-bug-workaround.css') }}" rel="stylesheet" type="text/css"/>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="{{ asset('bundles/coretemplate/assets/font-awesome-4.7.0/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/coretemplate/assets/jquery-ui/css/jquery-ui.min.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/coretemplate/assets/css/main.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/coretemplate/assets/css/custom.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/coretemplate/assets/css/reject.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/coretemplate/assets/select2-4.0.3/dist/css/select2.min.css') }}" rel="stylesheet" type="text/css"/>
    {% block css %}{% endblock %}
</head>

<body class="page">
    <div id="site-canvas">
        <div class="page--wrap">
            {% embed 'CoreTemplateBundle::top_menu_white.html.twig' %}{% endembed %}
            {% for flash_msg in app.session.flashbag.get('error') %}
                <div class="alert alert-danger alert-dismissable">
                    <button type="button" 
                    class="close" data-dismiss="alert" aria-hidden="true"></button>
                    <strong>Error: </strong>
                    {{ flash_msg }}
                </div>
                {% endfor %}

                {% for flash_msg in app.session.flashbag.get('success') %}
                <div class="alert alert-success alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
                    <strong>Success: </strong>
                    {{ flash_msg }}
                </div>
            {% endfor %}
            {% block content %}
            {% endblock %}
        </div>
        <footer class="site-footer">
            <div class="container">
                <a href="#">Hive <span>/ Copyright 2017</span></a>
                <ul class="site-footer--menu">
                    <li>
                        <a href="#">Getting started</a>
                    </li>
                    <li>
                        <a href="#">Help</a>
                    </li>
                    <li>
                        <a href="#">Careers</a>
                    </li>
                    <li>
                        <a href="#">Privacy</a>
                    </li>
                    <li>
                        <a href="#">Terms</a>
                    </li>
                    <li>
                        <a href="#">About</a>
                    </li>
                </ul>
            </div>
        </footer>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>
    <script src="{{ asset('bundles/coretemplate/assets/js/template.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/bootstrap-validator-master/validator.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/jquery/dist/jquery.min.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/bootstrap/js/bootstrap.min.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/jquery-ui/js/jquery-ui.min.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/js/jquery.placeholder.min.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/js/jquery.reject.min.js')}}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/js/main.js')}}" type="text/javascript"></script>
    <script src="https://masonry.desandro.com/masonry.pkgd.js"></script>
    <script src="{{ asset('bundles/coretemplate/assets/select2-4.0.3/dist/js/select2.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/coretemplate/assets/js/template.js') }}" type="text/javascript"></script>
    <script>
        jQuery(document).ready(function() {
            Template.init();
        });
    </script>
    {% embed 'CoreTemplateBundle::credit_modal.html.twig' %}{% endembed %}
    {% block js %}{% endblock %}
</body>
</html>
